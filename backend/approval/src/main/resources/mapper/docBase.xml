<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.approval.dao.DocMapper">
    <insert id="insertDoc">
        insert into
        doc_basc(doc_ttl,doc_file,doc_act_yn,doc_user_id,crt_date)
        values (#{doc_ttl},#{doc_file},"N",#{user_id},#{crt_date})
    </insert>

    <select id="selectDoc" resultType="com.example.approval.dto.retrieveAllOutputDto">
        select doc_no
             , doc_user_id
             , doc_ttl
             , crt_date
        from doc_basc
        where doc_user_id = #{user_id}
    </select>
</mapper>
